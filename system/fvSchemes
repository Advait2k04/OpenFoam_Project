FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
    grad(S1)	    cellLimited Gauss linear 1;
    div(phi,S1)     Gauss vanLeer;
}

divSchemes
{
    default         none;
    div(phi,U)      Gauss linearUpwindV default;
    div(phi,S1)     Gauss vanLeer;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}


